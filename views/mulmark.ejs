<link rel="stylesheet" href="css/viewans.css">
<section>
    <!--for demo wrap-->
    <h1>Answers</h1>
    
    <div class="tbl-header">
      <table cellpadding="0" cellspacing="0" border="0">
        <thead>
          <tr>
           
            <th>Mark</th>
            <th>Subject</th>
           
           
            </tr>
        </thead>
      </table>
    </div>
    <div class="tbl-content">
      <table cellpadding="0" cellspacing="0" border="0">
        <tbody>
          <% user.forEach(function(user){ %>
          <tr>
          
            <td><%= user.mark%></td>
            <td><%= user.subject%></td>
           </tr>
           <% }); %>
          </tbody>
      </table>
    </div>
  </section>
